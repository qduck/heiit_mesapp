{
  "presets": [
    "react-native",
    "module:metro-react-native-babel-preset"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "core-js": 2
      }
    ],
    [
      "import",
      {
        "libraryName": "@ant-design/react-native"
      }
    ]
  ]
}